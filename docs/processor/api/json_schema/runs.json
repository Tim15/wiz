{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "runs"
    ],
    "properties": {
      "runs": {
        "$id": "#/properties/runs",
        "type": "array",
        "title": "The Runs Schema",
        "items": {
          "$id": "#/properties/runs/items",
          "type": "object",
          "title": "The Items Schema",
          "required": [
            "runID"
          ],
          "properties": {
            "runID": {
              "$id": "#/properties/runs/items/properties/runID",
              "type": "string",
              "title": "The Runid Schema",
              "default": "",
              "examples": [
                "aafhueiwlahufil"
              ],
              "pattern": "^(.*)$"
            }
          }
        }
      }
    }
  }