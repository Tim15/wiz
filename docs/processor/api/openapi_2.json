{
  "swagger": "2.0",
  "info": {
    "title": "Wiz Processor API",
    "description": "A system for interfacing with Wiz Processors",
    "version": "1.0"
  },
  "host": "api.server.test",
  "basePath": "/",
  "schemes": ["http"],
  "paths": {
    "/runs": {
      "get": {
        "description": "Returns all of the runs this processor has executed in its lifetime. Remember that processors can be GCed after their runs are done, so this should not be used extensively",
        "parameters": [],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "./json_schema/runs.json"
            },
            "examples": {
              "metadata": {
                "$ref": "./raw_json/runs.json"
              }
            }
          }
        }
      }
    },
    "/": {
      "get": {
        "description": "Returns metadata about the processor, which identifies it to the Manager",
        "responses": {
          "200": {
            "description": "Metadata fetched successfully",
            "schema": {
              "$ref": "./json_schema/processor.json"
            },
            "examples": {
              "metadata": {
                "$ref": "./raw_json/processor.json"
              }
            }
          }
        }
      }
    }
  }
}
