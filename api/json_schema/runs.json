{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/hyper-schema#",
  "$id": "http://wiz-project.ml/runs.json",
  "type": "object",
  "title": "The processor Runs schema",
  "required": ["runs"],
  "properties": {
    "runs": {
      "$id": "#/properties/runs",
      "type": "array",
      "title": "The Runs Schema",
      "items": {
        "$id": "#/properties/runs/items",
        "type": "object",
        "title": "The Items Schema",
        "required": ["runID"],
        "properties": {
          "runID": {
            "$id": "#/properties/runs/items/properties/runID",
            "type": "string",
            "title": "The Runid Schema",
            "default": "",
            "examples": ["aafhueiwlahufil"],
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }
}
